{
  "name": "api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js",
    "prod-migration:generate": "NODE_ENV=production npx typeorm migration:generate ./src/database/migrations/$TableName -d ./dist/config/datasource.js",
    "local-migration:generate": "NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/database/migrations/$TableName -d ./src/config/datasource.ts",
    "migration:create": "NODE_ENV=production yarn run typeorm migration:create ./src/database/migrations/$TableName",
    "local-migration:run": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/config/datasource.ts",
    "prod-migration:run": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -d ./dist/config/datasource.js",
    "local-migration:revert": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/config/datasource.ts",
    "prod-migration:revert": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:revert -d ./dist/config/datasource.js",
    "local-migration:show": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js migration:show -d ./src/config/datasource.ts",
    "prod-migration:show": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:show -d ./dist/config/datasource.js",
    "local-migration:schema-sync": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js schema:sync -d ./src/config/datasource.ts",
    "prod-migration:schema-sync": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js schema:sync -d ./dist/config/datasource.js",
    "local-migration:schema-log": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js schema:log -d ./src/config/datasource.ts",
    "prod-migration:schema-log": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js schema:log -d ./dist/config/datasource.js",
    "local-migration:schema-drop": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli.js schema:drop -d ./src/config/datasource.ts",
    "prod-migration:schema-drop": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js schema:drop -d ./dist/config/datasource.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/cache-manager-ioredis": "^2.0.6",
    "@types/uuid": "^10.0.0",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis": "^2.1.0",
    "class-validator": "^0.14.1",
    "ioredis": "^5.4.1",
    "pg": "^8.13.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}